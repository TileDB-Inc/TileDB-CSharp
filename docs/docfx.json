{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "sources/**.csproj"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "README.md"
        ],
        "src": ".."
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "logo.svg"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "globalMetadataFiles": [],
    "globalMetadata" :
    {
      "_appTitle" : "TileDB C# API",
      "_appFooter" : "Copyright &copy; 2018-2023 TileDB Inc.",
      "_enableSearch": false
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
